<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>loginPage</title>
    <link rel="stylesheet" href="/styles/styles-login-page.css">
    <script id="applicationScript" src="/js/login-page.js"></script>
</head>

<body>
    <div id="loginPage">
        <div id="Viatours">
            <span>Viatours</span>
        </div>
        <div id="Home">
            <span><a href="/landing">Home</a></span>
        </div>
        <div id="Weather">
            <span><a href="#">Weather</a></span>
        </div>
        <div id="Signup">
            <span><a href="#">Sign Up</a></span>
        </div>
        <svg class="Line_1" viewBox="0 0 1480 1">
            <path id="Line_1" d="M 0 0 L 1480 0">
            </path>
        </svg>
        <div id="Group_9">
            <svg class="Rectangle_12">
                <rect id="Rectangle_12" rx="0" ry="0" x="0" y="0" width="100%" height="333">
                </rect>
            </svg>
            <div id="Contact">
                <span>Contact</span>
            </div>
            <div id="n_28_Queensberry_StreetNorth_M">
                <span>328 Queensberry Street,North Melbourne Vic3051,Australia<br />hi@viatours.com</span>
            </div>
            <div id="Copyright_Viatours_2024">
                <span>@Copyright Viatours 2024</span>
            </div>
        </div>
        <div id="Group_2">
            <div id="Group_1">
                <div id="Login">
                    <span>Login</span>
                </div>
                <svg class="Rectangle_1">
                    <rect id="Rectangle_1" rx="28" ry="28" x="0" y="0" width="136" height="56">
                    </rect>
                </svg>
            </div>
        </div>
        <svg class="Rectangle_13">
            <rect id="Rectangle_13" rx="31" ry="31" x="0" y="0" width="1125" height="573">
            </rect>
        </svg>
        <svg class="Rectangle_14">
            <rect id="Rectangle_14" rx="31" ry="31" x="0" y="0" width="687" height="448">
            </rect>
        </svg>
        <div id="Login_to_your_account">
            <span>Login to your account</span>
        </div>
        <div id="Group_10">
            <div id="Email_">
                <span>Email :</span>
            </div>
            <svg class="Rectangle_15">
                <rect id="Rectangle_15" rx="22" ry="22" x="0" y="0" width="523" height="44">
                </rect>
            </svg>
        </div>
        <div id="Password_">
            <span>Password :</span>
        </div>
        <svg class="Rectangle_15_">
            <rect id="Rectangle_15_" rx="22" ry="22" x="0" y="0" width="523" height="44">
            </rect>
        </svg>


        <form action="/login" method="post">
            <div id="Enter_your_password">
                <!-- <span>Enter your password</span> -->
                <input name="password" type="password" placeholder="Enter your password"
                    style="font-size: 22px; border: none;outline: none;"
                    onfocus="this.style.border='none'; this.style.outline='none';">
            </div>
            <div id="abcgmailcom">
                <!-- <span>abc@gmail.com</span> -->
                <input name="name" type="text" placeholder="abc@gmail.com"
                    style="font-size: 22px; border: none;outline: none;"
                    onfocus="this.style.border='none'; this.style.outline='none';">
            </div>

            <div id="btn">
                <svg class="Rectangle_16">
                    <rect id="Rectangle_16" rx="9" ry="9" x="0" y="0" width="520" height="57">
                    </rect>
                </svg>
                <div id="Login_Now_">
                    <!-- <span>Login Now </span> -->
                    <!-- <span></span> -->
                    <input id="Login_Now_Submit" type="submit" value="Login Now">
                </div>
            </div>

        </form>
        <div id="do_not_have_an_account_sign_up">
            <span>do not have an account? </span><span style="color:rgba(213,102,42,1);">sign up</span>
        </div>
    </div>
</body>

</html>